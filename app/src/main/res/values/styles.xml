<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="button_style">
        <item name="android:padding">@dimen/_6sdp</item>
        <item name="android:background">@drawable/button_desgin</item>
        <item name="android:layout_marginTop">@dimen/_8sdp</item>
        <item name="android:layout_marginLeft">@dimen/_30sdp</item>
        <item name="android:layout_marginRight">@dimen/_30sdp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/_14sdp</item>
    </style>
</resources>
